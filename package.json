{
  "name": "grav-theme-wynona",
  "version": "0.0.1",
  "description": "Grav theme for the Wynona restaurant in Toronto Canada",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bovisp/grav-theme-wynona.git"
  },
  "keywords": [
    "grav",
    "theme",
    "wynona",
    "toronto",
    "restaurant"
  ],
  "author": "Paul Bovis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bovisp/grav-theme-wynona/issues"
  },
  "homepage": "https://github.com/bovisp/grav-theme-wynona#readme",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.15",
    "tailwindcss": "^0.7.4"
  }
}
